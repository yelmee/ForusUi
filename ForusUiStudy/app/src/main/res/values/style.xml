<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="signInStyle">
        <item name="android:padding">20dp</item>
    </style>

    <style name="btnStyle">
        <item name="android:padding">15dp</item>
        <item name="android:layout_marginLeft">16dp</item>
        <item name="android:layout_marginRight">16dp</item>
        <item name="android:width">328dp</item>
        <item name="height">48dp</item>
        <item name="android:textSize">12dp</item>
    </style>

    <style name="warnTextStyle">
        <item name="android:textSize">10dp</item>
        <item name="android:layout_marginLeft">16dp</item>
        <item name="android:layout_marginRight">16dp</item>
        <item name="android:textColor">#FD1E1E</item>
    </style>

    <style name="textPadding">
        <item name="android:paddingRight">12dp</item>
    </style>

    <style name="iconStyle">
        <item name="android:layout_width">24dp</item>
        <item name="android:layout_height">24dp</item>
        <item name="background">@drawable/bg_round_et</item>
        <item name="android:layout_margin">10dp</item>
    </style>

    <style name="topNavTextStyle">
        <item name="android:textSize">20dp</item>
        <item name="android:layout_marginTop">19dp</item>
        <item name="android:layout_marginBottom">14dp</item>
    </style>

    <style name="TEXT_BOLD_TITLE_STYLE">
        <item name="android:textSize">18dp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="TEXT_SMALL_GRAY_STYLE">
        <item name="android:padding">3dp</item>
        <item name="android:textSize">12dp</item>
        <item name="android:textColor">@color/black</item>
    </style>

    <style name="Calendar"/>

    <style name="Calendar.HeaderDateText">
        <item name="android:textSize">20dp</item>
        <item name="android:textColor">#263238</item>
    </style>

    <style name="Calendar.CalendarViewStyle">
        <item name="dayHeight">60dp</item>
    </style>

    <style name="Calendar.ItemViewStyle">
        <item name="dayTextSize">14dp</item>
    </style>

    <style name="Day"/>
    <style name="Day.DayViewStyle">
        <item name="dayOfMonthHeight">25dp</item>
    </style>

    <style name="Day.DayViewStyle.TextStyle">
    <item name="dayOfMonthTextSize">20dp</item>
    </style>
    <style name="Rect"/>
    <style name="Rect.rectShape">
        <item name="termFrom"></item>
    </style>

    <style name="Rect.RectViewStyle">
        <item name="rectHeight">60dp</item>
    </style>

</resources>